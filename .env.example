# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/ai_assessment?schema=public"

# Redis (for BullMQ workers)
REDIS_URL="redis://localhost:6379"

# Upstash Redis (for rate limiting)
# For local development, use: http://localhost:6379
# For production, use your Upstash Redis URL
UPSTASH_REDIS_REST_URL="http://localhost:6379"
UPSTASH_REDIS_REST_TOKEN="local_development_token"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-change-in-production"

# Email (Nodemailer)
SMTP_HOST="smtp.example.com"
SMTP_PORT="587"
SMTP_USER="your-email@example.com"
SMTP_PASSWORD="your-smtp-password"
SMTP_FROM="noreply@example.com"

# Application
NODE_ENV="development"
